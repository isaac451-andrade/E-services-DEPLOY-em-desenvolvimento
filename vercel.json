{
  "builds": [
    {
      "src": "back-end/apiSetup/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10.6"
      }
    },
    {
      "src": "front-end/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "back-end/apiSetup/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "front-end/$1"
    }
  ]
}